{
    "rateLimit":  {
                      "limit":  120,
                      "windowSec":  60,
                      "mode":  "enforce"
                  },
    "headers":  {
                    "XContentTypeOptions":  "nosniff",
                    "ReferrerPolicy":  "no-referrer",
                    "XFrameOptions":  "DENY"
                },
